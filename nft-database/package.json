    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "https": "^1.0.0",
    "nodemon": "^2.0.15",
    "p-limit": "^3.1.0",
    "redis": "^4.0.6",
    "toobusy-js": "^0.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.26",
    "@types/toobusy-js": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
