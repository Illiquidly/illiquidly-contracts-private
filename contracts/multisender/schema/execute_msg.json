          "type": "string"
        },
        "amount": {
          "$ref": "#/definitions/Uint128"
        }
      }
    },
    "Cw721Coin": {
      "type": "object",
      "required": [
        "address",
        "token_id"
      ],
      "properties": {
        "address": {
          "type": "string"
        },
        "token_id": {
          "type": "string"
        }
      }
    },
    "TokenToSend": {
      "oneOf": [
        {
          "type": "object",
          "required": [
            "cw20_coin"
          ],
          "properties": {
