              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "to_send": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/TokenToSend"
              }
            }
          }
        }
      },
      "additionalProperties": false
    }
  ],
  "definitions": {
    "Cw20Coin": {
      "type": "object",
      "required": [
        "address",
        "amount"
      ],
      "properties": {
